{
  "kind": "ingress",
  "flavor": "nginx_k8s",
  "disabled": true,
  "version": "1.0",
  "metadata": {
    "annotations": {}
  },
  "spec": {
    "private": false,
    "basic_auth": false,
    "grpc": false,
    "domains": {},
    "rules": {},
    "force_ssl_redirection": true
  },
  "inputs": {
    "kubernetes_details": {
      "resource_name": "aws-cluster",
      "resource_type": "kubernetes_cluster"
    },
    "cert_manager_details": {
      "resource_name": "aws-certm",
      "resource_type": "cert_manager"
    },
    "artifactories": {
      "resource_name": "aws-artifact",
      "resource_type": "artifactory"
    },
    "kubernetes_node_pool_details": {
      "resource_name": "aws-np",
      "resource_type": "kubernetes_node_pool"
    }
  }
}