{
  "flavor": "aws",
  "metadata": {
    "labels": {
      "deliveryType": "MANUAL"
    }
  },
  "kind": "service",
  "disabled": true,
  "version": "1.0",
  "spec": {
    "type": "application",
    "enable_host_anti_affinity": false,
    "restart_policy": "Always",
    "pod_distribution_enabled": false,
    "cloud_permissions": {
      "aws": {
        "enable_irsa": false
      }
    },
    "runtime": {
      "args": [],
      "command": [],
      "autoscaling": {
        "scaling_on": "CPU",
        "min": 1,
        "max": 1,
        "cpu_threshold": "50"
      },
      "ports": {
        "ports1": {
          "port": "8080",
          "protocol": "tcp"
        }
      },
      "size": {
        "cpu": "300m",
        "memory": "1Gi",
        "cpu_limit": "1000m",
        "memory_limit": "5Gi"
      },
      "health_checks": {
        "readiness_check_type": "None",
        "liveness_check_type": "None"
      }
    },
    "env": {
      "LOG_LEVEL": "INFO"
    },
    "release": {
      "image": "${blueprint.self.artifacts.aws-project-type-1155708878-aws-service}"
    }
  },
  "inputs": {
    "kubernetes_details": {
      "resource_name": "aws-cluster",
      "resource_type": "kubernetes_cluster",
      "output_name": "attributes"
    },
    "cloud_account": {
      "resource_name": "aws-cloud",
      "resource_type": "cloud_account"
    },
    "artifactories": {
      "resource_name": "aws-artifact",
      "resource_type": "artifactory"
    },
    "vpa_details": {
      "resource_name": "aws-vpa",
      "resource_type": "vpa"
    },
    "kubernetes_node_pool_details": {
      "resource_name": "aws-np",
      "resource_type": "kubernetes_node_pool",
      "output_name": "attributes"
    }
  },
  "uiProps": {}
}